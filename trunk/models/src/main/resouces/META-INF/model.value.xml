<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="modelValue.paserFinder" class="cn.geodata.models.value.ModelValueParserFinder">
		<property name="parsers">
			<map>
				<entry key="literal.normal"><ref bean="literal.normal" />
				</entry>
				<entry key="complex.gml"><ref bean="complex.gml" />
				</entry>
				<entry key="complexReference.gml"><ref bean="complexReference.gml" />
				</entry>
			</map>
		</property>
		
		<property name="encoders">
			<map>
				<entry key="literal.normal"><ref bean="literal.normal" />
				</entry>
				<entry key="complex.gml"><ref bean="complex.gml" />
				</entry>
				<entry key="complexReference.gml"><ref bean="complexReference.gml" />
				</entry>
			</map>
		</property>
		
		<property name="defaultLiteralEncoder"><value>literal.normal</value></property>
		<property name="defaultLiteralParser"><value>literal.normal</value></property>
		<property name="defaultComplexEncoder"><value>complex.gml</value></property>
		<property name="defaultComplexParser"><value>complex.gml</value></property>
		<property name="defaultComplexReferenceEncoder"><value>complexReference.gml</value></property>
		<property name="defaultComplexReferenceParser"><value>complexReference.gml</value></property>
	</bean>
	
	<bean id="literal.normal" class="cn.geodata.models.value.literal.LiteralValueParser">
		<property name="parsers">
			<map>
				<entry key="literal.string"><ref bean="literal.string" />
				</entry>
				<entry key="literal.integer"><ref bean="literal.integer" />
				</entry>
				<entry key="literal.long"><ref bean="literal.long" />
				</entry>
				<entry key="literal.float"><ref bean="literal.float" />
				</entry>
				<entry key="literal.double"><ref bean="literal.double" />
				</entry>
			</map>
		</property>
		
		<property name="encoders">
			<map>
				<entry key="literal.string"><ref bean="literal.string" />
				</entry>
				<entry key="literal.integer"><ref bean="literal.integer" />
				</entry>
				<entry key="literal.long"><ref bean="literal.long" />
				</entry>
				<entry key="literal.float"><ref bean="literal.float" />
				</entry>
				<entry key="literal.double"><ref bean="literal.double" />
				</entry>
			</map>
		</property>
	</bean>
	
	<bean id="complex.gml" class="cn.geodata.models.value.gml.GMLComplexValueParser">
	</bean>
	
	<bean id="complexReference.gml" class="cn.geodata.models.value.gml.GMLComplexReferenceParser">
	</bean>
	
	<bean id="literal.string" class="cn.geodata.models.value.literal.StringParser">
	</bean>

	<bean id="literal.integer" class="cn.geodata.models.value.literal.IntegerParser">
	</bean>

	<bean id="literal.long" class="cn.geodata.models.value.literal.LongParser">
	</bean>
	
	<bean id="literal.float" class="cn.geodata.models.value.literal.FloatParser">
	</bean>
	
	<bean id="literal.double" class="cn.geodata.models.value.literal.DoubleParser">
	</bean>
	
</beans>
