<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="mapMap" class="cn.geodata.models.wetland.actions.map.Map"></bean>
	<bean id="mapChart" class="cn.geodata.models.wetland.actions.map.Chart">
		<property name="envi" ref="geoEnvironment" />
	</bean>
	<bean id="geoEnvironment" class="cn.geodata.models.wetland.actions.map.GeoEnvironment">
		<property name="geoserver" value="D:/mfeng/save/java/wps/service/wetland/tomcat-6.0.16" />
		<property name="wetland" value="webapps/geoserver/data/data/wetland/wetland.shp" />
	</bean>
	<bean id="chartManager" class="cn.geodata.models.wetland.actions.map.ChartManager">
		<property name="dataPath" value="/wetland.data/" />
	</bean>
	<bean id="chartImage" class="cn.geodata.models.wetland.actions.map.ChartImage">
		<property name="manager" ref="chartManager"></property>
		<property name="api" value="http://chart.apis.google.com/chart?"></property>
		<property name="width" value="900"></property>
		<property name="height" value="400"></property>
	</bean>
</beans>
